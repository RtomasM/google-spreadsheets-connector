<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<connector name="Google Spreadsheets">
    <properties>
    <connectivity-support>true</connectivity-support>
    <OAuth-authentication>true</OAuth-authentication>
    <metadata>static</metadata>
    <datasense-enabled>true</datasense-enabled>
    <transformer>false</transformer>
  </properties>
    <oauthConfig>
        <required caption="Name" group="Basic Settings" javaType="java.lang.String" name="name" type="name"> </required>
        <required caption="Consumer Key" group="General" javaType="java.lang.String" name="consumerKey" prefix="" type="string"> </required>
        <required caption="Consumer Secret" group="General" javaType="java.lang.String" name="consumerSecret" prefix="" type="string"> </required>
        <required name="objectStore-ref" prefix=""> </required>
        <required name="ref" prefix=""> </required>
        <required name="oauth-callback-config" prefix=""> </required>
        <required name="ref" prefix=""> </required>
    </oauthConfig>
    <message-processors>
        <authorize name="Authorize">
            <datamapper input="" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes/>
            <childElements/>
        </authorize>
        <unauthorize name="Unauthorize">
            <datamapper input="" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes/>
            <childElements/>
        </unauthorize>
        <get-all-spreadsheets name="Get all spreadsheets">
            <datamapper input="List&lt;Spreadsheet&gt;" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes/>
            <childElements/>
        </get-all-spreadsheets>
        <create-spreadsheet name="Create spreadsheet">
            <datamapper input="" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Title" group="General" javaType="java.lang.String" name="title" type="string"> </required>
            </attributes>
            <childElements/>
        </create-spreadsheet>
        <get-all-worksheets name="Get all worksheets">
            <datamapper input="List&lt;Worksheet&gt;" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Spreadsheet" group="General" javaType="java.lang.String" name="spreadsheet" type="string"> </required>
            </attributes>
            <childElements/>
        </get-all-worksheets>
        <create-worksheet name="Create worksheet">
            <datamapper input="Worksheet" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Spreadsheet" group="General" javaType="java.lang.String" name="spreadsheet" type="string"> </required>
                <required caption="Title" group="General" javaType="java.lang.String" name="title" type="string"> </required>
                <required caption="Row Count" group="General" javaType="int" name="rowCount" type="integer"> </required>
                <required caption="Col Count" group="General" javaType="int" name="colCount" type="integer"> </required>
            </attributes>
            <childElements/>
        </create-worksheet>
        <delete-worksheet name="Delete worksheet">
            <datamapper input="" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Spreadsheet" group="General" javaType="java.lang.String" name="spreadsheet" type="string"> </required>
                <required caption="Worksheet" group="General" javaType="java.lang.String" name="worksheet" type="string"> </required>
            </attributes>
            <childElements/>
        </delete-worksheet>
        <update-worksheet-metadata name="Update worksheet metadata">
            <datamapper input="" output="Boolean"/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Spreadsheet" group="General" javaType="java.lang.String" name="spreadsheet" type="string"> </required>
                <required caption="Worksheet" group="General" javaType="java.lang.String" name="worksheet" type="string"> </required>
            </attributes>
            <childElements/>
