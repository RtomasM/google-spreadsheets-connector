            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Spreadsheet" group="General" javaType="java.lang.String" name="spreadsheet" type="string"> </required>
                <required caption="Title" group="General" javaType="java.lang.String" name="title" type="string"> </required>
            </attributes>
            <childElements/>
        </get-worksheet-by-title>
        <purge-worksheet name="Purge worksheet">
            <datamapper input="" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Spreadsheet" group="General" javaType="java.lang.String" name="spreadsheet" type="string"> </required>
                <required caption="Worksheet" group="General" javaType="java.lang.String" name="worksheet" type="string"> </required>
            </attributes>
            <childElements/>
