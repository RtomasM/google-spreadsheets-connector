        <get-cell-range-as-csv name="Get cell range as csv">
            <datamapper input="" output="Integer"/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Spreadsheet" group="General" javaType="java.lang.String" name="spreadsheet" type="string"> </required>
                <required caption="Worksheet" group="General" javaType="java.lang.String" name="worksheet" type="string"> </required>
            </attributes>
            <childElements/>
        </get-cell-range-as-csv>
        <search name="Search">
            <datamapper input="List&lt;Row&gt;" output=""/>
            <auto-paging>false</auto-paging>
            <query-support>false</query-support>
            <attributes>
                <required caption="Spreadsheet" group="General" javaType="java.lang.String" name="spreadsheet" type="string"> </required>
                <required caption="Worksheet" group="General" javaType="java.lang.String" name="worksheet" type="string"> </required>
                <required caption="Query" group="General" javaType="java.lang.String" name="query" type="string"> </required>
